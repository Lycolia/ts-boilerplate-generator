{
  "name": "@lycolia/ts-boilerplate-generator-cli",
  "version": "0.6.0",
  "description": "ts-server-boilerplate project Generator CLI",
  "keywords": [
    "typescript",
    "generator",
    "generate",
    "boilerplate",
    "project",
    "scaffolding",
    "linux",
    "node.js",
    "react",
    "next.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Lycolia/ts-boilerplate-generator-cli"
  },
  "bin": {
    "tsg": "dist/src/index.js"
  },
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "dev": "node -r ts-node/register --inspect-brk ./src/index.ts",
    "type-check": "tsc --noEmit && eslint \"**/*.{js,ts}\" .",
    "build": "tsc",
    "test": "jest --watch",
    "headless-test": "jest",
    "test-all": "./test-all.zsh",
    "coverage": "jest --coverage"
  },
  "author": "Lycolia",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.1",
    "commander": "^8.0.0",
    "prompts": "^2.4.1",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.2",
    "@types/prompts": "^2.0.14",
    "@types/validate-npm-package-name": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
