{
  "name": "@lycolia/ts-boilerplate-generator-cli",
  "version": "0.22.0",
  "description": "ts-server-boilerplate project Generator CLI",
  "keywords": [
    "typescript",
    "generator",
    "generate",
    "boilerplate",
    "project",
    "scaffolding",
    "linux",
    "node.js",
    "react",
    "next.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Lycolia/ts-boilerplate-generator-cli"
  },
  "bin": {
    "tsg": "dist/index.js"
  },
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "npm run build && node --inspect-brk dist/index.js",
    "type-check": "tsc --noEmit && eslint \"**/*.{js,ts}\" .",
    "build": "rm -Rf dist/ && swc src/ --config-file .swcrc_build -d dist/",
    "test": "jest --watch",
    "headless-test": "jest --colors",
    "coverage": "jest --coverage"
  },
  "author": "Lycolia",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "prompts": "^2.4.2",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@lycolia/eslint-config": "^0.9.1",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.103",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.8.10",
    "@types/prompts": "^2.4.7",
    "@types/validate-npm-package-name": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
