{
  "name": "@lycolia/ts-boilerplate-generator-cli",
  "version": "0.4.5",
  "description": "ts-server-boilerplate project Generator CLI",
  "keywords": [
    "typescript",
    "generator",
    "generate",
    "boilerplate",
    "project",
    "scaffolding",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Lycolia/ts-boilerplate-generator-cli"
  },
  "bin": {
    "tsg": "./dist/src/index.js"
  },
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "npx ts-node ./src/index.ts",
    "dev": "node -r ts-node/register --inspect-brk ./src/index.ts",
    "lint": "npx tsc --noEmit && npx eslint \"**/*.{js,ts}\" --quiet --fix",
    "build": "npx tsc",
    "test": "npx jest"
  },
  "author": "Lycolia",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "prompts": "^2.4.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/prompts": "^2.0.9",
    "@types/validate-npm-package-name": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
