{
  "name": "@lycolia/ts-boilerplate-generator-cli",
  "version": "0.8.7",
  "description": "ts-server-boilerplate project Generator CLI",
  "keywords": [
    "typescript",
    "generator",
    "generate",
    "boilerplate",
    "project",
    "scaffolding",
    "linux",
    "node.js",
    "react",
    "next.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Lycolia/ts-boilerplate-generator-cli"
  },
  "bin": {
    "tsg": "dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "npm run build && node --inspect-brk dist/index.js",
    "type-check": "tsc --noEmit && eslint \"**/*.{js,ts}\" .",
    "build": "rm -Rf dist/ && swc src/ -d dist/",
    "test": "jest --watch",
    "headless-test": "jest",
    "test-all": "./test-all.zsh",
    "coverage": "jest --coverage"
  },
  "author": "Lycolia",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.1",
    "commander": "^8.0.0",
    "prompts": "^2.4.1",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.108",
    "@swc/jest": "^0.2.5",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/prompts": "^2.0.14",
    "@types/validate-npm-package-name": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.3.1",
    "jest-watch-typeahead": "^1.0.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  }
}
