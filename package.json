{
  "name": "@lycolia/ts-boilerplate-generator-cli",
  "version": "0.20.0",
  "description": "ts-server-boilerplate project Generator CLI",
  "keywords": [
    "typescript",
    "generator",
    "generate",
    "boilerplate",
    "project",
    "scaffolding",
    "linux",
    "node.js",
    "react",
    "next.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Lycolia/ts-boilerplate-generator-cli"
  },
  "bin": {
    "tsg": "dist/index.js"
  },
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "npm run build && node --inspect-brk dist/index.js",
    "type-check": "tsc --noEmit && eslint \"**/*.{js,ts}\" .",
    "build": "rm -Rf dist/ && swc src/ --config-file .swcrc_build -d dist/",
    "test": "jest --watch",
    "headless-test": "jest",
    "test-all": "./test-all.zsh",
    "coverage": "jest --coverage"
  },
  "author": "Lycolia",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.0.0",
    "commander": "^10.0.0",
    "prompts": "^2.4.1",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@lycolia/eslint-config": "^0.2.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.6",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.4",
    "@types/prompts": "^2.0.14",
    "@types/validate-npm-package-name": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.2.0",
    "jest-watch-typeahead": "^2.2.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
